    "eslint": "8.57.0",
    "prisma": "5.20.0",
    "tsup": "8.3.0",
    "tsx": "4.19.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@fastify/swagger": "9.1.0",
    "@fastify/swagger-ui": "5.0.1",
    "@prisma/client": "5.20.0",
    "dotenv": "16.4.5",
    "fastify": "5.0.0",
    "zod": "3.23.8"
  }
